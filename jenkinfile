pipeline {
    agent any
    
    stages {
        stage('checkout') {
            steps {
                git 'https://github.com/satyasandeep48/jenkins-java-project-1-rahm.git' 
            }
        }
        stage('compile') {
            steps {
                sh "mvn compile"
            }
        }
        stage('test') {
            steps {
                sh 'mvn test'
            }
        }
        stage('artifact') {
            steps {
                sh 'mvn clean package'
            }
        }
        stage('deploy') {
           steps {
             sh 'mvn deploy'
            }
        }
    }
}
